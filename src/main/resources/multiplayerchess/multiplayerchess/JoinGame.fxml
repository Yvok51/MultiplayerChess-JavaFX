<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="multiplayerchess.multiplayerchess.client.ui.JoinGameController">
   <children>
      <TextField layoutX="225.0" layoutY="358.0" prefHeight="85.0" prefWidth="350.0" promptText="Enter Game Code">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button layoutX="422.0" layoutY="492.0" mnemonicParsing="false" onAction="#onJoinGame" prefHeight="85.0" prefWidth="175.0" text="Join Game">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button layoutX="199.0" layoutY="492.0" mnemonicParsing="false" onAction="#onBack" prefHeight="85.0" prefWidth="175.0" text="Back">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
